package mypackage;

public class Type_cast {

	public static void main(String[] args) {
		int p=100;
		byte q=(byte) p; //Begger data type to smallaer data type need to type cast
		System.out.println(q);
		//Reverse senario
		byte m=120;
		int n=m;
		System.out.println(n);
		
		// Another example 
	float j=5.5f;
	
	int k=(int) j; //Type_Casting here 
	double k1=j;
	System.out.println(k1);
	
		
	}

}
